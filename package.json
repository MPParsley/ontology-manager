{
  "name": "ontology-editor",
  "version": "1.0.0",
  "description": "Semantic Web, RDF ontology editor",
  "author": "Zazuko GmbH",
  "private": true,
  "scripts": {
    "dev": "node -r dotenv/config node_modules/.bin/nuxt dev . dotenv_config_path=docker-app-dev/.env",
    "dev-api": "nodemon --watch api --watch trifid --exec 'node -r dotenv/config' node_modules/.bin/nuxt dev . dotenv_config_path=docker-app-dev/.env",
    "build": "node -r dotenv/config node_modules/.bin/nuxt build . dotenv_config_path=docker-app-dev/.env",
    "start": "node -r dotenv/config node_modules/.bin/nuxt start . dotenv_config_path=docker-app-dev/.env",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "test": "node -r dotenv/config node_modules/.bin/jest . dotenv_config_path=test/.env"
  },
  "dependencies": {
    "@nuxtjs/apollo": "^4.0.0-rc2",
    "@nuxtjs/auth": "^4.5.2",
    "@nuxtjs/axios": "^5.3.2",
    "@nuxtjs/toast": "^3.0.1",
    "@octokit/rest": "^15.12.0",
    "@rdfjs/to-ntriples": "^1.0.0",
    "apicache": "^1.2.3",
    "apollo-cache-inmemory": "^1.2.10",
    "apollo-client": "^2.4.2",
    "apollo-link": "^1.2.3",
    "apollo-link-http": "^1.5.5",
    "express-jwt": "^5.3.1",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.9.2",
    "isomorphic-fetch": "^2.2.1",
    "nuxt": "^2.0.0",
    "rdf-ext": "^1.1.1",
    "rdf-parser-n3": "^1.1.1",
    "string-to-stream": "^1.1.1",
    "trifid": "^2.1.0",
    "trifid-handler-fetch": "^1.3.1",
    "vue-apollo": "3.0.0-beta.19"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.6.0",
    "babel-plugin-dynamic-import-node": "^2.1.0",
    "bulma": "^0.7.1",
    "dotenv": "^6.0.0",
    "eslint": "^5.6.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.22.1",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^4.0.0",
    "glob": "^7.1.3",
    "glob-promise": "^3.4.0",
    "jest": "^23.6.0",
    "jsdom": "^12.0.0",
    "knex": "^0.15.2",
    "node-sass": "^4.9.3",
    "pg": "^7.4.3",
    "puppeteer": "^1.8.0",
    "sass-loader": "^7.1.0"
  },
  "engines": {
    "node": ">=8.6",
    "npm": ">=6.0"
  }
}
